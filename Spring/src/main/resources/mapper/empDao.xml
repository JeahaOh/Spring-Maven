<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.spring.emp.dao.empDao">
	<select id="empList" resultType="Object">
		SELECT
			ENAME
			, COMM
			, HIREDATE
			, EMPNO
			, MGR
			, JOB
			, DEPTNO
			, SAL
		FROM EXAM_MANAGER.EMP
		<!-- FROM EMP -->
	</select>

</mapper>