<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.study.spring.db.dao.MembDao">
	<select id="membList" resultType="Map">
		<!-- 	
		SELECT 
			mno
		FROM p1_memb
		 -->
