<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="memb">
	<select id="membList" resultType="map">
		SELECT 
			mno,
			name,
			email,
			pwd,
			tel,
			cdt
		FROM p1_memb
	</select>
</mapper>
